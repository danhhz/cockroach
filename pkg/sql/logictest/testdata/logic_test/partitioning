    CONSTRAINT "primary" PRIMARY KEY (a ASC, b ASC, c ASC),
    FAMILY "primary" (a, b, c)
) PARTITION BY LIST (a) (
    PARTITION foo (VALUES (TODO)) PARTITION BY RANGE (b) (
