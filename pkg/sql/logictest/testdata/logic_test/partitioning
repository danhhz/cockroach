statement ok
CREATE TABLE subpartition (
  a INT, b INT, c INT, PRIMARY KEY (a, b, c)
) PARTITION BY LIST (a) (
  PARTITION foo (VALUES (0)) PARTITION BY RANGE (b) (
    PARTITION bar VALUES LESS THAN (VALUES (1)) PARTITION BY RANGE (c) (
      PARTITION baz VALUES LESS THAN (VALUES (2))
    )
  )
)

query TT
SHOW CREATE TABLE subpartition
----
subpartition CREATE TABLE subpartition (
    a INT NOT NULL,
